<h1>Listing product_qualities</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Quality code</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_qualities.each do |product_quality| %>
  <tr>
    <td><%= product_quality.product_id %></td>
    <td><%= product_quality.quality_code %></td>
    <td><%= product_quality.description %></td>
    <td><%= link_to 'Show', product_quality %></td>
    <td><%= link_to 'Edit', edit_product_quality_path(product_quality) %></td>
    <td><%= link_to 'Destroy', product_quality, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

