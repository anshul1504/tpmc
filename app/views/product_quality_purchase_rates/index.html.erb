<h1>Listing product_quality_purchase_rates</h1>

<table>
  <tr>
    <th>Product quality</th>
    <th>Purchase rate</th>
    <th>Start date</th>
    <th>End date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_quality_purchase_rates.each do |product_quality_purchase_rate| %>
  <tr>
    <td><%= product_quality_purchase_rate.product_quality.description %></td>
    <td><%= product_quality_purchase_rate.purchase_rate %></td>
    <td><%= product_quality_purchase_rate.start_date.to_s(:long) %></td>
    <td><%= product_quality_purchase_rate.end_date.to_s(:long) %></td>
    <td><%= link_to 'Show', product_quality_purchase_rate %></td>
    <td><%= link_to 'Edit', edit_product_quality_purchase_rate_path(product_quality_purchase_rate) %></td>
    <td><%= link_to 'Destroy', product_quality_purchase_rate, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

